{
  "name": "sequelize-typescript-migration-engine",
  "version": "0.0.5",
  "description": "Typescript based sequelize migration engine",
  "main": "dist/sequelize-typescript-migration-engine.js",
  "umd:main": "dist/sequelize-typescript-migration-engine.umd.js",
  "module": "dist/sequelize-typescript-migration-engine.mjs",
  "files": [
    "dist",
    "readme.md"
  ],
  "dependencies": {
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "jest": "^26.4.2",
    "microbundle": "^0.12.0",
    "pg": "^8.5.1",
    "prettier": "^2.0.4",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "microbundle -o dist/ --sourcemap false --compress false",
    "dev": "microbundle watch -o dist/ --sourcemap false --compress false",
    "test": "jest --coverage --rootDir src",
    "testapp": "jest --coverage --rootDir testapp",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feyzullahyildiz/sequelize-typescript-migration-engine"
  },
  "keywords": ["sequelize", "migration", "typescript"],
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist",
      "coverage"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "author": "Feyzullah YILDIZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feyzullahyildiz/sequelize-typescript-migration-engine/issues"
  },
  "homepage": "https://github.com/feyzullahyildiz/sequelize-typescript-migration-engine#readme"
}
